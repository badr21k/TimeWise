run = "bash start.sh"
modules = ["nix", "nodejs-20", "php-8.2", "postgresql-16"]

[nix]
channel = "stable-24_05"

[deployment]
run = ["sh", "-c", "bash start.sh"]
deploymentTarget = "cloudrun"

[[ports]]
localPort = 3001
externalPort = 3001

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 42591
externalPort = 3000

[[ports]]
localPort = 45605
externalPort = 3002

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Web Server"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "PORT=5000 bash start.sh"
waitForPort = 5000

[[workflows.workflow]]
name = "Web Server"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "PORT=5000 bash start.sh"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[agent]
expertMode = true
